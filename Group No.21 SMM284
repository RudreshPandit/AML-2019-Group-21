    "Y = np.asarray([0.5, 0.50, 0.5,  0.5, 0.1,  0.3])\n",
    "#Algo and parameter values\n",
    "algo = 'GD'\n",
    "w_init = 2.1\n",
    "b_init = 4.0\n",
    "#parameter min and max values- to plot update rule\n",
    "w_min = -7\n",
    "w_max = 5\n",
    "b_min = -7\n",
    "b_max = 5\n",
