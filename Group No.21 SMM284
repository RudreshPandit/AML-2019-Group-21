    "    for i in range(iter):\n",
    "        x_l,y_l,loss_l,x_l_g,y_l_g,x,y=g_d(0.001,i)\n",
    "        cost_function=l_f(x,y)\n",
    "        cost_list.append(cost_function)\n",
    "        \n",
    "    return cost_list"
   ]
  },
  {
