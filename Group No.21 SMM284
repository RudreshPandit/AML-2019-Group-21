   "outputs": [],
   "source": [
    "iteration = 10000\n",
    "sizes = []\n",
    "steps = []\n",
    "x_min = []\n",
    "y_min = []\n",
    "losses = []\n",
    "for i in l_s:\n",
    "    x_l, y_l, loss_l, x_l_g, y_l_g, x, y = g_d(\n",
    "        i, iteration)\n",
    "    sizes.append(i)\n",
    "    #losses.append(min(loss_list))\n",
    "    steps.append(len(x_l_g))\n",
    "    x_min.append(min(x_l_g))\n",
    "    y_min.append(min(y_l_g))\n",
    "\n",
    "    losses.append(l_f(x, y))\n",
    "    new = pd.DataFrame({\n",
    "        'loss': losses,\n",
    "        'no_steps': steps,\n",
    "        'x_list': x_min,\n",
    "        'y_list': y_min,\n",
    "        'size': sizes\n",
    "    })"
   ]
