   "source": [
    "l_s = np.arange(0.0001,0.01,0.00001)\n",
    "len(l_s)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2019-07-11T23:18:35.379916Z",
